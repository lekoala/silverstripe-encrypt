{
    "name": "lekoala/silverstripe-encrypt",
    "description": "Encrypt data extension for SilverStripe",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "encrypt",
        "module"
    ],
    "license": "MIT",
    "authors": [{
        "name": "LeKoala",
        "email": "thomas@lekoala.be"
    }],
    "require": {
        "php": ">7.2",
        "silverstripe/framework": "^4.1",
        "tuupola/base62": "^2.0",
        "paragonie/sodium_compat": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "LeKoala\\SilverStripeEncrypt\\": "src/",
            "LeKoala\\SilverStripeEncrypt\\Tests\\": "tests/"
        }
    },
    "extra": {
        "installer-name": "encrypt"
    },
    "support": {
        "issues": "https://github.com/lekoala/silverstripe-encrypt/issues"
    },
    "scripts": {
        "lint": "phpcs code/ tests/",
        "lint-clean": "phpcbf code/ tests/",
        "test": "phpunit -v"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
